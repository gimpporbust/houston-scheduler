<h3>
  Tickets
  <!-- <button id="sort_tickets" class="btn">Update Sequence</button> -->
  
  &nbsp;
  <small>Queuing Discipline:</small>
  <select id="queuing_discipline">
    {{#each queuingDisciplines}}
      <option>{{name}}</option>
    {{/each}}
  </select>
  
</h3>

<div class="headings">
  <div class="task-description">Summary</div>
  <div class="task-effort">Effort <small>(Hours)</small></div>
  <div class="task-value">Value <small>(Dollars)</small></div>
</div>

<ul id="tickets"></ul>

<h3>
  Schedule
  
  &nbsp;
  <small>Resources:</small>
  <input type="number" id="resources_count" value="3" style="width:60px" />
</h3>

<div id="schedule"></div>
<div id="schedule_axis"></div>
